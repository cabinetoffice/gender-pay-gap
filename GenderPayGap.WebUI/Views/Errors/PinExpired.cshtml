@model GenderPayGap.WebUI.ErrorHandling.PinExpiredException

@{
    ViewBag.Title = "ERROR: Your PIN has expired";
}

<div class="grid-row">
    <div class="column-two-thirds">
        <div class="govuk-box-highlight">
            <h1 class="bold-large">Your PIN has expired</h1>
        </div>
        <div class="subsection panel-indent">
            <div class="bold-small">Get a replacement PIN</div>
        </div>
        <p></p>
        <a class="button" href="@(Url.Action("ManageOrganisationsGet", "ManageOrganisations"))" role="button">Continue</a>
    </div>
</div>
